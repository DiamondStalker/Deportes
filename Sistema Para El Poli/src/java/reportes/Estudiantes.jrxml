	<subDataset name="Lista_Estudiantes" uuid="1b6cbd60-af45-4aed-acbc-da62723a86a8">
		<queryString language="SQL">
			<![CDATA[SELECT
     estudiante.`id` AS estudiante_id,
     estudiante.`nombre` AS estudiante_nombre,
     estudiante.`apellido` AS estudiante_apellido,
     deporte.`nombre` AS deporte_nombre,
     categoria.`descripcion_categoria` AS categoria_descripcion_categoria
FROM
     `estudiante` estudiante INNER JOIN `matricula` matricula ON estudiante.`id` = matricula.`id`
     INNER JOIN `deporte_categoria_horario` deporte_categoria_horario ON matricula.`codigo_relacion` = deporte_categoria_horario.`codigo_relacion`
     INNER JOIN `categoria` categoria ON deporte_categoria_horario.`codigo_categoria` = categoria.`codigo_categoria`
     INNER JOIN `deporte` deporte ON deporte_categoria_horario.`codigo_deporte` = deporte.`codigo_deporte`]]>
		</queryString>
