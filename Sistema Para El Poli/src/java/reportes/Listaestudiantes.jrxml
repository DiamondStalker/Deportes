	<queryString>
		<![CDATA[SELECT
     estudiante.`id` AS estudiante_id,
     estudiante.`nombre` AS estudiante_nombre,
     estudiante.`apellido` AS estudiante_apellido,
     deporte.`nombre` AS deporte_nombre,
     categoria.`descripcion_categoria` AS categoria_descripcion_categoria
FROM
     `deporte` deporte INNER JOIN `deporte_categoria_horario` deporte_categoria_horario ON deporte.`codigo_deporte` = deporte_categoria_horario.`codigo_deporte`
     INNER JOIN `categoria` categoria ON deporte_categoria_horario.`codigo_categoria` = categoria.`codigo_categoria`
     INNER JOIN `matricula` matricula ON deporte_categoria_horario.`codigo_relacion` = matricula.`codigo_relacion`
     INNER JOIN `estudiante` estudiante ON matricula.`id` = estudiante.`id`]]>
	</queryString>
