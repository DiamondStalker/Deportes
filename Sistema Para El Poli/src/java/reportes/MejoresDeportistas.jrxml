	<queryString>
		<![CDATA[SELECT estudiante.id,SUM(seguimiento.Nota),AVG(seguimiento.Nota) as final,
     matricula.`codigo_matricula` AS matricula_codigo_matricula,
     estudiante.`id` AS estudiante_id,
     estudiante.`nombre` AS estudiante_nombre,
     estudiante.`apellido` AS estudiante_apellido,
     estudiante.`fecha_nacimiento` AS estudiante_fecha_nacimiento,
     deporte.`nombre` AS deporte_nombre,
     categoria.`descripcion_categoria` AS categoria_descripcion_categoria,
     horario.`descripcion_horario` AS horario_descripcion_horario
FROM
     `matricula` matricula INNER JOIN `seguimiento` seguimiento ON matricula.`codigo_matricula` = seguimiento.`codigo_matricula`
     INNER JOIN `estudiante` estudiante ON matricula.`id` = estudiante.`id`
     INNER JOIN `deporte_categoria_horario` deporte_categoria_horario ON matricula.`codigo_relacion` = deporte_categoria_horario.`codigo_relacion`
     INNER JOIN `deporte` deporte ON deporte_categoria_horario.`codigo_deporte` = deporte.`codigo_deporte`
     INNER JOIN `categoria` categoria ON deporte_categoria_horario.`codigo_categoria` = categoria.`codigo_categoria`
     INNER JOIN `horario` horario ON deporte_categoria_horario.`codigo_horario` = horario.`codigo_horario`
     HAVING final >= 4.5]]>
	</queryString>
