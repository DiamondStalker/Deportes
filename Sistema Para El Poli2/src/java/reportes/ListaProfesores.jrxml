	<queryString>
		<![CDATA[SELECT
     persona.`id` AS persona_id,
     persona.`nombre` AS persona_nombre,
     persona.`apellido` AS persona_apellido,
     persona.`telefono` AS persona_telefono,
     persona.`celular` AS persona_celular,
     deporte.`nombre` AS deporte_nombre,
     categoria.`descripcion_categoria` AS categoria_descripcion_categoria
FROM
     `persona` persona INNER JOIN `entrenador` entrenador ON persona.`id` = entrenador.`id`
     INNER JOIN `clase` clase ON entrenador.`id` = clase.`id`
     INNER JOIN `deporte_categoria_horario` deporte_categoria_horario ON clase.`codigo_relacion` = deporte_categoria_horario.`codigo_relacion`
     INNER JOIN `deporte` deporte ON deporte_categoria_horario.`codigo_deporte` = deporte.`codigo_deporte`
     INNER JOIN `categoria` categoria ON deporte_categoria_horario.`codigo_categoria` = categoria.`codigo_categoria`
WHERE deporte.`nombre` = $P{Nombre_deporte}]]>
	</queryString>
